{
  "name": "zemble-plugin-supabase",
  "version": "0.0.3",
  "description": "",
  "type": "module",
  "keywords": [
    "zemble",
    "zemble-plugin",
    "@zemble"
  ],
  "trustedDependencies": [ "supabase" ],
  "dependencies": {
    "@zemble/core": "workspace:*",
    "@zemble/bun": "workspace:*",
    "zemble-plugin-auth": "workspace:*",
    "@zemble/routes": "workspace:*",
    "@zemble/graphql": "workspace:*",
    "@supabase/ssr": "^0.0.10",
    "graphql": "^16.8.1",
    "@supabase/supabase-js": "^2.39.1"
  },
  "scripts": {
    "dev": "zemble-dev plugin.ts",
    "lint": "eslint .",
    "typecheck": "bun run codegen && tsc --noEmit",
    "codegen": "graphql-codegen",
    "supabase-codegen": "supabase gen types typescript --project-id \"nptqmmaxmynahsgfuvhn\" --schema public > types/supabase.generated.ts"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "supabase": "^1.123.4",
    "@graphql-codegen/add": "^5.0.0",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-resolvers": "^4.0.1",
    "@tsconfig/bun": "^1.0.1"
  },
  "peerDependencies": {
    "typescript": "^5.2.2"
  },
  "module": "plugin.ts",
  "main": "plugin.ts"
