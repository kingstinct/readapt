{
  "extends": "@tsconfig/bun/tsconfig.json",
  "compilerOptions": {
    "noUncheckedIndexedAccess": true,
    "strictNullChecks": true,
    "verbatimModuleSyntax": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "types": [ "bun-types" ]
  },
  "exclude": [
    "**/*.generated/*.ts",
    "**/*.generated.ts"
  ]
}
