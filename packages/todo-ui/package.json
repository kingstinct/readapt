    "zemble-plugin-auth-expo": "workspace:*",
    "urql": "^4.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
